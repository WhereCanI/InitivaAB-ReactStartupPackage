{
	"compilerOptions": {
		"sourceMap": true,
		"noImplicitAny": false,
		"removeComments": true,
		"esModuleInterop": true,
		"module": "commonjs",
		"target": "es6",
		"jsx": "react",
		"forceConsistentCasingInFileNames": true,
		"noUnusedParameters": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitThis": true,
		"noImplicitReturns": false
	},
	"include": [
		"src/**/*"
	]
}